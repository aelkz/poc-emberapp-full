<h1>List of products</h1>
<ul>
  {{#each model as |product|}}
    <li>
      {{#link-to 'products.view-product' product}}
        {{product.name}}
      {{/link-to}}
      <button {{action 'delete' product}}>Delete</button>
    </li>
  {{/each}}
</ul>
{{#link-to 'products.new-product'}}New Product{{/link-to}}