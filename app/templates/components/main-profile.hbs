<div class="row">
  <div class="profile-aside col-md-3">
    <img class="avatar" src="{{profile.pictureUrl}}" />
    <h3>{{profile.name}}</h3>
    <h4>{{profile.nickname}}</h4>
    <h4>{{profile.email}}</h4>
  </div>
  <div class="col-md-9">
    <div class="page-header">
      <h1>Profile</h1>
    </div>
    <pre>
      {{profile.body}}
    </pre>
  </div>
</div>
